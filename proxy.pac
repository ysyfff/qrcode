function FindProxyForURL(url, host) {
    var D = "DIRECT";
	var H = "PROXY hkproxy.cn1.global.ctrip.com:8080";
	var A = "PROXY proxy9.cn1.global.ctrip.com:8080";
	var M = "PROXY 100.64.1.2:8080";
	var N = "PROXY 10.14.7.11:80";
    
    var i = 0;

	// Direct Proxy
    if(dnsDomainIs(host, ".ctripcorp.com") || dnsDomainIs(host, ".ctriptravel.com"))
    {
        return D;
    }

	// Custom Proxy
    for (i in CustomProxyList) {
        if(shExpMatch(url, CustomProxyList[i])) return H;
    }
	
	// HK Proxy
    for (i in HKProxyList) {
        if(shExpMatch(url, HKProxyList[i])) return H;
    }

	// OPS JinPin Proxy Domain
    for (i in OpsProxyList) {
        if (shExpMatch(url, OpsProxyList[i])) return H;
    }

	// elong Proxy
    for (i in ELProxyList) {
        if(shExpMatch(url, ELProxyList[i])) return M;
    }

	// HuoChePiao Proxy (tany S36222)
    for (i in HcpProxyList) {
        if(shExpMatch(url, HcpProxyList[i])) return N;
    }
	
    return D;
}

